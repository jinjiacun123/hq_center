    private double ldzc;        //'流动资产',
    private double gdzc;        //'固定资产',
    private double wxzc;        //'无形资产',
    private int    cqtz;        //'股东人数',
    private double ldfz;        //'流动负债',
    private double cqfz;        //'少数股权',
    private double zbgjj;       //'公积金',
